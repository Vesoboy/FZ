@model DataBaseSite



<form asp-action="ActiveFalse" asp-controller="home" method="post">

    <div class="form-group">
        <input asp-for="Id" class="d-none" />
    </div>

    <div class="form-group">
        <input type="text" asp-for="Url" class="d-none" />
    </div>

    <div class="form-group">
        <input type="text" asp-for="Message" class="d-none" />
    </div>

    <div class="form-group">
        <input type="checkbox" asp-for="Active" class="d-none" />
    </div>

    <div>
		<input type="submit" value="Выкл сайт" class="align-middle btn btn-outline-danger " />
    </div>

</form>